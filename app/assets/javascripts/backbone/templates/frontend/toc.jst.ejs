<li class="level-0">
	<header class="pane-close pane--header">
		<h2 class="pane--title">Table of Contents</h2>
	</header>
	<section class="pane--content">
		<ul class="nav-index">
			<% parts.each( function(part) { %> 
				<li>
					<a href="#part/<%= part.get('id') %>"><%= part.get('label') %></a>
				</li>
			<% }) %>
		</ul>
	</section>
</li>
<li class="level-1">
	<header class="pane--header">
		<h2 class="pane--title">Articles</h2>
	</header>
	<section class="pane--content pane--vertical-stage">
		<div class="parts">
			<% parts.each( function(part) { %> 
			<div id="toc-part-<%= part.get('id') %>" class="part">
				<% _.each(texts.ByPartGrouped(part.get('id'), 5), function(textGroup) { %> 
					<ul class="nav-index nav-index-col">
						<% _.each(textGroup, function(text) { %>
							<li><a href="/debates/<%= text.get('id') %>"><%= text.get('title') %></a></li>	
						<% }) %>
					</ul>
				<% }) %>
			</div>
			<% }) %>
		<div class="parts">
	</section>
</li>